{
  "name": "carrot",
  "version": "1.0.0",
  "description": "carrot 是一款帮助你更好使用百度网盘的浏览器插件",
  "main": "index.js",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=./build/tsconfig.json NODE_ENV=development webpack --config ./build/webpack/webpack.config.dev.ts -w",
    "build": "npm run build:zip",
    "build:prod": "cross-env TS_NODE_PROJECT=./build/tsconfig.json NODE_ENV=production webpack --config ./build/webpack/webpack.config.prod.ts -p",
    "build:zip": "npm run build:prod&&cross-env TS_NODE_PROJECT=./build/tsconfig.json ts-node ./build/zip.ts",
    "lint": "npm run lint:ts&&npm run lint:scss",
    "lint:ts": "tslint -p tsconfig.json ./src/**/*.ts",
    "lint:scss": "",
    "test": "jest"
  },
  "keywords": [
    "百度网盘",
    "百度云",
    "carrot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AepKill/Carrot.git"
  },
  "author": "AEPKILL",
  "license": "MIT",
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.1",
    "@types/cli-color": "^0.3.29",
    "@types/copy-webpack-plugin": "^4.4.0",
    "@types/html-webpack-plugin": "^2.30.2",
    "@types/jest": "^22.2.0",
    "@types/webpack": "^3.8.9",
    "@types/webpack-merge": "^4.1.2",
    "awesome-typescript-loader": "^3.5.0",
    "clean-webpack-plugin": "^0.1.19",
    "cli-color": "^1.2.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.3",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "style-loader": "^0.20.3",
    "style-webpack-plugin": "^1.0.4",
    "stylelint": "^9.1.1",
    "stylelint-config-sass-guidelines": "^4.2.0",
    "stylelint-config-scss": "github:P233/stylelint-config-SCSS",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.5.0",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@types/archiver": "^2.1.0",
    "@types/chrome": "0.0.58",
    "@types/clipboard": "^2.0.0",
    "@types/events": "^1.2.0",
    "@types/node-sass": "^3.10.32",
    "@types/perfect-scrollbar": "^1.3.0",
    "archiver": "^2.1.1",
    "clipboard": "^2.0.0",
    "deferred-factory": "^1.0.2",
    "events": "^2.0.0",
    "perfect-scrollbar": "^1.3.0"
  }
}
